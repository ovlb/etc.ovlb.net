extends layout

block content
  -
    var posts = artikel;

  main.o-main-content.o-archive.js-main-content.js-is-index
    each post in posts
      article.o-card.a--above__parent.js-article(data-slug=post.slug)
        a(class="a--above" href=post.slug)
        header.c-card__header
          h2.c-card__header.c-sub-headline.c-card__headline= post.title
        p.c-card__abstract= post.abstract
        footer.c-card__footer
          p.p--small.c-card__meta #{post.type}â€”#{post.date}
